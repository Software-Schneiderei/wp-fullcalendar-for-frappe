{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "software-schneiderei/fullcalendar-for-frappe",
	"version": "0.2.1",
	"title": "Fullcalendar",
	"category": "widgets",
	"icon": "calendar-alt",
	"description": "Fullcalendar frontend for fetching available appointments from an API",
	"example": {},
	"supports": {
		"align": true,
		"dimensions": {
			"aspectRatio": true,
			"minHeight": true
		},
		"color": {
			"background": true,
			"text": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"textAlign": true
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true
		}
	},
	"textdomain": "fullcalendar-for-frappe",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js",
	"attributes": {
		"eventsPerTimeslotUrl": {
			"type": "string"
		},
		"eventsPerDayUrl": {
			"type": "string"
		},
		"calendarOptions": {
			"type": "string"
		}
	}
}
